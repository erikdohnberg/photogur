<h1>Add a picture to
	<br>Bitmaker Labs
</h1>
<!-- <form action="/pictures" method="POST"> -->
<%= form_for @picture do |f| %>

	<label>Artist</label>
	<!-- <p><input type="text" name="artist"/></p> -->
	<p><%= f.text_field "artist" %></p>

	<label>Title</label>
	<!-- <p><input type="text" name="title"/></p> -->
	<p><%= f.text_field "title" %></p>
	
	<label>Image URL</label>
	<!-- <p><input type="text" name="url"/></p> -->
	<p><%= f.text_field "url" %></p>

	<!-- <p><input type="submit" value="Bit It!"/></p> -->
	<p><%= f.submit "Bit It!" %></p>
	
<!-- </form> -->
<% end %>