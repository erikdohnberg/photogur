<h1>Add a picture to
	<br>Bitmaker Labs
</h1>
<%= form_for @picture do |f| %>

	<p>
		<label>Artist</label><br>
		<%= f.text_field "artist" %>
	</p>
	
	<p>
		<label>Title</label><br>
		<%= f.text_field "title" %>
	</p>

	<p>
		<label>Image URL</label><br>
		<%= f.text_field "url" %>
	</p>

	<p><%= f.submit "Bit it!!" %></p>
	
<% end %>